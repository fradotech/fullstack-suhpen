{
  "name": "fullstack-nest-react",
  "version": "0.0.1",
  "description": "fullstack-nest-react",
  "author": "Fradotech",
  "private": true,
  "license": "Fradotech",
  "scripts": {
    "format": "prettier --write \"@server/**/*.ts\" \"client/**/*{.ts,.tsx,.js,.css}\"",
    "lint": "eslint \"{@server,client}/**/*.ts\" --fix",
    "folderslint": "folderslint",
    "prepare": "husky install",
    "server:prebuild": "rimraf dist",
    "server:build": "nest build",
    "server": "nest start --watch",
    "server:debug": "nest start --debug --watch",
    "server:prod": "node dist/@server/main",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./@server/database/data-source.ts",
    "typeorm:reset": "yarn schema:drop && yarn migrate && yarn server",
    "migrate": "yarn typeorm migration:run",
    "migrate:generate": "sh -c 'yarn typeorm migration:generate @server/database/migrations/$0'",
    "migrate:revert": "yarn typeorm migration:revert",
    "schema:drop": "yarn typeorm schema:drop",
    "client": "vite",
    "client:build": "tsc && vite build",
    "client:preview": "vite preview"
  },
  "dependencies": {
    "@ant-design/charts": "^1.4.2",
    "@ant-design/icons": "^5.0.1",
    "@json2csv/plainjs": "^6.1.2",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/cache-manager": "^2.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/typeorm": "^9.0.1",
    "@sentry/node": "^7.41.0",
    "@sentry/react": "^7.41.0",
    "@sentry/tracing": "^7.41.0",
    "@types/axios": "^0.14.0",
    "antd": "^5.2.0",
    "antd-colorpicker": "^1.0.0",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "cache-manager": "^5.2.3",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "folderslint": "^1.2.0",
    "helmet": "^7.0.0",
    "js-file-download": "^0.4.12",
    "mysql": "^2.18.1",
    "nest-raven": "^9.1.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.11.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-query": "^3.39.3",
    "react-responsive": "^9.0.2",
    "react-router-dom": "^6.8.0",
    "redis": "^4.6.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.10",
    "typeorm-naming-strategies": "^4.1.0",
    "typeorm-transactional-cls-hooked": "^0.1.21"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.4.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.18",
    "@types/passport": "^1.0.11",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.1.7",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.2.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "29.5.0",
    "jest-cli": "^29.5.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
