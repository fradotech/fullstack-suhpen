{
  "name": "fullstack-nest-react",
  "version": "0.0.1",
  "description": "",
  "author": "Fradotech",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "format": "prettier --write \"@server/**/*.ts\" \"client/**/*{.ts,.tsx,.js,.css}\"",
    "lint": "eslint \"{@server,client}/**/*.ts\" --fix",
    "prepare": "husky install",
    "server:prebuild": "rimraf dist",
    "server:build": "nest build",
    "server": "nest start",
    "server:dev": "nest start --watch",
    "server:debug": "nest start --debug --watch",
    "server:prod": "node dist/@server/main",
    "typeorm": "yarn server:build && npx typeorm -d dist/@server/database/data-source.js",
    "migrate": "yarn typeorm migration:run",
    "migrate:generate": "sh -c 'yarn typeorm migration:generate @server/database/migrations/$0'",
    "migrate:revert": "yarn typeorm migration:revert",
    "client:dev": "webpack-dev-server --open --hot --config webpack.config.js --mode=development",
    "client:build": "webpack --config webpack.config.js --mode=production",
    "client": "webpack-dev-server --mode=production --config webpack.config.js "
  },
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-typescript": "^7.12.13",
    "@json2csv/plainjs": "^6.1.2",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/typeorm": "^9.0.1",
    "@sentry/node": "^7.41.0",
    "@sentry/react": "^7.41.0",
    "@sentry/tracing": "^7.41.0",
    "antd": "^5.2.0",
    "axios": "^1.3.4",
    "babel-plugin-react-css-modules": "^5.2.6",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "js-file-download": "^0.4.12",
    "mysql": "^2.18.1",
    "nest-raven": "^9.1.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-responsive": "^9.0.2",
    "react-router-dom": "^6.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.4.0",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.10",
    "typeorm-naming-strategies": "^4.1.0",
    "typeorm-transactional-cls-hooked": "^0.1.21"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.10.4",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/babel__traverse": "^7.18.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.4.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.18",
    "@types/passport": "^1.0.11",
    "@types/react-css-modules": "^4.6.2",
    "@types/react-router-dom": "^5.1.7",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "autoprefixer": "^10.2.3",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "file-loader": "^6.1.1",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^8.0.3",
    "jest": "29.5.0",
    "jest-cli": "^29.5.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "style-loader": "^2.0.0",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.2",
    "typescript": "^5.0.2",
    "url-loader": "^4.1.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
